<html lang="en">
<head>
  <meta charset="utf-8"/>
  <title>DomoDance</title>
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  <script>
    var randoNum = 0;
    var random = Math.floor(Math.random() * 5);
    randoNum = random;
    console.log(randoNum);
  </script>
  <link rel="shortcut icon" href="/assets/img/favicon.png"/>
  <link rel="stylesheet" type="text/css" href="/assets/style.css"/>
</head>
<body>
  <nav><a href="/login"><img id="logo" src="/assets/img/face.png" alt="face logo"/></a>
    <div class="navlink"><a href="/logout">Log out</a></div>
  </nav>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.34/browser.min.js"></script>
  <script src="/assets/maker.js" type="text/babel"></script>
  <h3 id="refresh">Refresh the page!</h3>
  <section id="domos">
    <div class="domo">
			{{#if domos}}
					<div class="domo"><img src="/assets/img/domoface.jpeg" alt="domo.face" class="domoFace"/>
						<h3 class="domoName">Name: {{domos.randoNum.name}}</h3>
						<h3 class="domoAge">Age: {{domos.randoNum.age}}</h3>
						<h3 class="domoColor">Color: {{domos.randoNum.color}}</h3>
					</div>
			{{else}}
				<h3 class="emptyDomo">No Domos yet</h3>
			{{/if}}
    </div>
  </section>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/babel-core/5.8.34/browser.min.js"></script>
  <script src="/assets/maker.js" type="text/babel"></script>
</body>
</html>